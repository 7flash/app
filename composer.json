{
	"name": "phundament/app",
	"description": "Yii 2 Application Template",
	"keywords": ["phundament", "yii2", "framework", "application template", "vagrant"],
	"homepage": "http://www.phundament.com/",
	"type": "project",
	"license": "BSD-3-Clause",
    "authors":[
        {
            "name":"Tobias Munk",
            "email":"schmunk@usrbin.de",
            "homepage":"http://www.phundament.com"
        }
    ],
	"support": {
		"issues": "https://github.com/yiisoft/yii2/issues?state=open",
		"forum": "http://www.yiiframework.com/forum/",
		"wiki": "http://www.yiiframework.com/wiki/",
		"irc": "irc://irc.freenode.net/yii",
		"source": "https://github.com/yiisoft/yii2"
	},
	"minimum-stability": "stable",
	"require": {
		"php": ">=5.4.0",
        "yiisoft/yii2-composer": "dev-master",
		"yiisoft/yii2-dev": "dev-master",
        "swiftmailer/swiftmailer": "5.*",
		"schmunk42/yii2-packaii": "dev-master",
		"schmunk42/yii2-templay": "dev-develop",
        "twbs/bootstrap": "3.*"
    },
	"require-dev": {
        "codeception/codeception": "1.8.*",
        "codeception/specify": "*",
        "codeception/verify": "*",
        "yiisoft/yii2-coding-standards": "dev-master",
        "schmunk42/yii2-giiant": "dev-develop"
	},
	"scripts": {
		"post-create-project-cmd": [
			"yii\\composer\\Installer::setPermission"
		]
	},
	"config": {
		"process-timeout": 1800
	},
	"extra": {
		"writable": [
			"app/runtime",
			"web/assets"
		],
		"executable": [
			"yii"
		]
	}
}
