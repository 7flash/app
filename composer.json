{
    "name": "phundament/app",
    "description": "Yii 2 Application Template",
    "keywords": ["phundament", "yii2", "framework", "application template", "vagrant"],
    "homepage": "http://www.phundament.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Tobias Munk",
            "email": "schmunk@usrbin.de",
            "homepage": "http://www.phundament.com"
        }
    ],
    "support": {
        "issues": "https://github.com/phundament/app/issues?state=open",
        "forum": "https://groups.google.com/forum/#!forum/phundament-dev",
        "source": "https://github.com/phundament/app"
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/schmunk42/yii2-package-browser.git"
        }
    ],

    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2-composer": "*",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "dmstr/yii2-migrate-command": "*",
        "dektrium/yii2-user": "*",
        "schmunk42/yii2-packaii": "*"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "schmunk42/yii2-giiant": "dev-develop"
    },
    "autoload": {
        "psr-4": {
            "console\\": "./console"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\Installer::setPermission",
            "console\\helpers\\ComposerCommand::init"
        ]
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "writable": [
            "backend/runtime",
            "backend/web/assets",
            "frontend/runtime",
            "frontend/web/assets"
        ]
    }
}
