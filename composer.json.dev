{
	"name": "phundament/app",
	"description": "Yii 2 Application Template",
	"keywords": ["phundament", "yii2", "framework", "application template", "vagrant"],
	"homepage": "http://www.phundament.com/",
	"type": "yii2-application",
	"license": "BSD-3-Clause",
    "authors":[
        {
            "name":"Tobias Munk",
            "email":"schmunk@usrbin.de",
            "homepage":"http://www.phundament.com"
        }
    ],
	"support": {
		"issues": "https://github.com/yiisoft/yii2/issues?state=open",
		"forum": "http://www.yiiframework.com/forum/",
		"wiki": "http://www.yiiframework.com/wiki/",
		"irc": "irc://irc.freenode.net/yii",
		"source": "https://github.com/yiisoft/yii2"
	},
	"minimum-stability": "dev",
    "repositories":[
        {
            "type": "git",
            "url": "https://github.com/schmunk42/yii2-package-browser.git"
        }
    ],
	"require": {
		"php": ">=5.4.0",
		"yiisoft/yii2-dev": "*",
		"yiisoft/yii2-bootstrap": "*",
		"yiisoft/yii2-swiftmailer": "*",
		"nineinchnick/yii2-usr": "*",
		"schmunk42/yii2-packaii": "dev-master",
        "twbs/bootstrap": "3.*",
        "swiftmailer/swiftmailer": "*"
    },
	"require-dev": {
		"yiisoft/yii2-codeception": "*",
		"codeception/codeception": "*",
		"codeception/specify": "*",
		"codeception/verify": "*",
		"yiisoft/yii2-debug": "*",
		"yiisoft/yii2-gii": "*",
        "yiisoft/yii2-coding-standards": "*",
        "schmunk42/yii2-giiant": "dev-develop",
        "phpunit/phpunit-mock-objects": "1.2.3"
	},
	"scripts": {
		"post-create-project-cmd": [
			"yii\\composer\\Installer::setPermission"
		]
	},
	"config": {
		"process-timeout": 1800
	},
	"extra": {
		"writable": [
			"runtime",
			"web/assets"
		],
		"executable": [
			"yii"
		]
	}
}
